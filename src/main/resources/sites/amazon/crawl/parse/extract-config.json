{
  "version": "0.0.1",
  "portalUrls": [],
  "sqlResourcePrefix": "sites/amazon/crawl/parse/sql/crawl",
  "syncConfig": {
    "batchSize": 60,
    "cacheTTL": "10m"
  },
  "extractRules": [
    {
      "id": 1,
      "name": "asin",
      "urlPattern": ".+/dp/.+",
      "minContentSize": 500000,
      "minNumNonBlankFields": 20,
      "sqlTemplate": "x-asin.sql",
      "collection": "asin_sync_utf8mb4"
    }
  ]
}
